{
    "name": "brainwave/cache",
    "type": "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords": ["narrowspark", "brainwave", "cache", "apc", "apcu", "redis", "mongo"],
    "license": "MIT",
    "homepage": "http://github.com/n-brainwave/config",
    "authors": [
        {
          "name": "Daniel Bannert",
          "email": "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role": "Developer"
        }
    ],
    "require": {
        "php"              : ">=5.4.0",
        "predis/predis"    : "~1.0",
        "pimple/pimple"    : "~3.0.0",
        "brainwave/config" : "self.version"
    },
    "autoload": {
        "psr-4": {
            "Brainwave\\Cache\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.4-dev"
        }
    },
    "suggest": {
        "predis/predis": "Flexible and feature-complete PHP client library for Redis",
        "ext-apc"        : "*",
        "ext-apcu"       : "*",
        "ext-redis"      : "*",
        "ext-mongo"      : "*",
        "ext-memcached"  : "*",
        "ext-pdo"        : "*",
        "ext-pdo_sqlite" : "*",
        "ext-pdo_mysql"  : "*",
        "ext-pdo_pgsql"  : "*"
    },
    "minimum-stability": "dev"
}
