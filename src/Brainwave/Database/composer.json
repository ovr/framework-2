{
    "name": "brainwave/conifg",
    "type": "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords": ["narrowspark", "brainwave", "database", "slim", "SQL", "MySQL", "MSSQL", "SQLite", "Sybase", "Oracle", "Google Cloud SQL"],
    "license": "MIT",
    "homepage": "http://github.com/n-brainwave/database",
    "authors": [
        {
          "name": "Daniel Bannert",
          "email": "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "pimple/pimple": "2.*",
        "brainwave/cache": "self.version",
        "brainwave/config": "self.version",
        "brainwave/support": "self.version",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-0": {
            "Brainwave\\Database": ""
        }
    },
    "target-dir": "Brainwave/Database",
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.2-dev"
        }
    },
    "suggest": {
        "ext-pdo-mysql": "For MySQL or MariaDB databases",
        "ext-pdo_sqlsrv": "For MSSQL databases on Windows platform",
        "ext-pdo_dblib": "For MSSQL databases on Liunx/UNIX platform",
        "ext-pdo-pqsql": "For PostgreSQL databases",
        "ext-pdo-sqlite": "For SQLite databases"
    },
    "minimum-stability": "dev"
}
