{
    "name": "narrowspark/framework",
    "type": "library",
    "description": "The Brainwave Framework.",
    "keywords": ["framework", "narrowspark"],
    "license": "MIT",
    "homepage": "http://github.com/narrowspark/framework",
    "authors": [
        {
          "name": "Daniel Bannert",
          "email": "d.bannert@anolilab.de",
          "homepage": "http://www.narrowspark.de",
          "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "pimple/pimple": "2.*",
        "nesbot/carbon": "1.9.*",
        "patchwork/utf8": "1.1.*",
        "monolog/monolog": "1.9.1",
        "filp/whoops": "1.2.*@dev",
        "guzzlehttp/streams": "1.*",
        "swiftmailer/swiftmailer": "5.2.*",
        "jeremeamia/superclosure": "1.*",
        "classpreloader/classpreloader": "1.*",
        "intahwebz/lowmemoryclassloader": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "codeception/codeception": "*",
    },
    "autoload": {
        "psr-0": {
            "Brainwave": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.1-dev"
        }
    },
    "suggest": {
        "league/plates": "Brainwave provides the integration with the plates templating engine",
        "ext-mcrypt": "Required for HTTP cookie encryption"
    },
    "minimum-stability": "dev"
}