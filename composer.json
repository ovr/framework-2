{
    "name": "narrowspark/framework",
    "type": "library",
    "description": "Narrowspark Framework for Creative People.",
    "keywords": ["framework", "narrowspark", "slim", "router", "brainwave"],
    "license": "MIT",
    "homepage": "http://github.com/narrowspark/framework",
    "authors": [
        {
          "name": "Daniel Bannert",
          "email": "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "pimple/pimple": "2.*",
        "league/plates": "2.*",
        "nesbot/carbon": "1.9.*",
        "patchwork/utf8": "1.1.*",
        "monolog/monolog": "1.9.1",
        "filp/whoops": "1.2.*@dev",
        "guzzlehttp/streams": "1.*",
        "swiftmailer/swiftmailer": "5.2.*",
        "jeremeamia/superclosure": "1.*",
        "classpreloader/classpreloader": "1.*",
        "intahwebz/lowmemoryclassloader": "dev-master"
    },
    "replace": {
        "brainwave/conifg": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-0": {
            "Brainwave": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.1-dev"
        }
    },
    "suggest": {
        "league/plates": "Narrowspark provides the integration with the plates templating engine",
        "ext-mcrypt": "Mcrypt generates the next best secure CSRF tokens and it is required for HTTP cookie encryption",
        "ext-openssl": "OpenSSL generates the best secure CSRF tokens.",
        "jamesmoss/toml": "Narrowspark has support for TOML config",
        "symfony/yaml" : "Narrowspark has support for YML config",
        "classpreloader/classpreloader": "If opcache_is_script_cached exists, classpreloader will speed up your application."
    },
    "minimum-stability": "dev"
}