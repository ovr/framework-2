{
    "name": "narrowspark/framework",
    "type": "framework",
    "description": "Narrowspark Framework for Creative People.",
    "keywords": ["framework", "narrowspark", "http", "request", "response", "brainwave"],
    "license": "MIT",
    "homepage": "https://github.com/narrowspark/framework",
    "authors": [
        {
          "name": "Daniel Bannert",
          "email": "d.bannert@anolilab.de",
          "homepage": "http://www.anolilab.de",
          "role": "Developer"
        }
    ],
    "require": {
        "php"                                : ">=5.4.0",
        "ext-mcrypt"                         : "*",
        "ext-openssl"                        : "*",
        "pimple/pimple"                      : "~3.0",
        "predis/predis"                      : "~1.0",
        "nesbot/carbon"                      : "~1.9",
        "filp/whoops"                        : "~1.1.3",
        "patchwork/utf8"                     : "~1.1",
        "league/plates"                      : "~3.1.0",
        "nikic/fast-route"                   : "~0.3.0",
        "symfony/console"                    : "~2.6.1",
        "symfony/process"                    : "~2.6.1",
        "league/fractal"                     : "~0.10.0",
        "vlucas/phpdotenv"                   : "~1.0.9",
        "monolog/monolog"                    : "~1.11.0",
        "stecman/symfony-console-completion" : "~0.4.4",
        "propel/propel"                      : "2.0.0-alpha4",
        "league/flysystem"                   : "~1.0",
        "doctrine/annotations"               : "~1.2.1",
        "swiftmailer/swiftmailer"            : "~5.2",
        "symfony/http-foundation"            : "~2.6.1",
        "jeremeamia/superclosure"            : "~1.0.1",
        "classpreloader/classpreloader"      : "~1.0.2"
    },
    "replace": {
        "brainwave/cache"                    : "self.version",
        "brainwave/conifg"                   : "self.version",
        "brainwave/cookie"                   : "self.version",
        "brainwave/crypter"                  : "self.version",
        "brainwave/event"                    : "self.version",
        "brainwave/exception"                : "self.version",
        "brainwave/log"                      : "self.version",
        "brainwave/mail"                     : "self.version",
        "brainwave/session"                  : "self.version",
        "brainwave/support"                  : "self.version",
        "brainwave/translator"               : "self.version",
        "brainwave/view"                     : "self.version"
    },
    "require-dev": {
        "phpunit/phpunit"                    : "~4.2",
        "mockery/mockery"                    : "~0.9",
        "rackspace/php-opencloud"            : "~1.8"
    },
    "autoload": {
        "psr-4": {
            "Brainwave\\"                    : "src/Brainwave/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brainwave\\Tests\\"             : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master"                     : "0.9.4-dev"
        }
    },
    "suggest": {
        "jamesmoss/toml": "Narrowspark has support for TOML files",
        "symfony/yaml"  : "Narrowspark has support for YML files",
        "symfony/debug" : "You can use symfony debug if you dont like whoops"
    },
    "minimum-stability" : "dev"
}
